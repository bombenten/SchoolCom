#include <stdio.h>
main(){
	int a,b,c;
	printf("Enter A : ");
	scanf("%d",&a);
	printf("Enter B : ");
	scanf("%d",&b);
	printf("Enter C : ");
	scanf("%d",&c);
	
	if(a>b){
		if(a>c){
			printf("Max is A");
		}
	}
	if(b>a){
		if(b>c){
			printf("Max is B");
		}
	}
	if(c>a){
		if(c>b){
			printf("Max is C");
		}
	}

}

